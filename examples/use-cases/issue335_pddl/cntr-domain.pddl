          (NOT (AT-AGENT EXPLORER1 L8 L3))
          (AT-AGENT EXPLORER1 L8 L4))))
    (FORALL (?O - OBSTACLE)
      (WHEN
        (AND
          (NOT (COMPROMISED ?O))
